{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "https://www.thebluedrops.com/(.*)",
      "destination": "https://thebluedrops.com/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/home",
      "destination": "/"
    },
    {
      "source": "/home/",
      "destination": "/"
    },
    {
      "source": "/up1bt",
      "destination": "/index.html"
    },
    {
      "source": "/up3bt",
      "destination": "/index.html"
    },
    {
      "source": "/up6bt",
      "destination": "/index.html"
    },
    {
      "source": "/upig1bt",
      "destination": "/index.html"
    },
    {
      "source": "/upig3bt",
      "destination": "/index.html"
    },
    {
      "source": "/upig6bt",
      "destination": "/index.html"
    },
    {
      "source": "/dws1",
      "destination": "/index.html"
    },
    {
      "source": "/dws2",
      "destination": "/index.html"
    },
    {
      "source": "/dw3",
      "destination": "/index.html"
    },
    {
      "source": "/ftr",
      "destination": "/index.html"
    },
    {
      "source": "/admin",
      "destination": "/index.html"
    },
    {
      "source": "/thankyou",
      "destination": "/index.html"
    },
    {
      "source": "/lpw",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}